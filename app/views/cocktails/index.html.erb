<div class="container">
<%= link_to "COCKTAIL +",new_cocktail_path, class:"btn btn-primary"  %>
  <h1 class="text-center" >All Cocktails</h1>
  <div class="row">
      <% @cocktails.each do |cocktail| %>
        <div class="col-xs-12 col-sm-6">
          <div class="cards">
            <div class="main-card">
            <div class="card-body" style="background-image:url('https://images.unsplash.com/photo-1509669803555-fd5edd8d5a41?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=800&q=60')">
              <h2><%=link_to cocktail.name, cocktail_path(cocktail) %></h2>
              <%= link_to "+ Dose",new_cocktail_dose_path(cocktail), class:"btn"  %>
              </div>
            </div>
          </div>
       </div>
    <% end %>
    </div>
</div>




